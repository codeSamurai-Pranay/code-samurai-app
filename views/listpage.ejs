<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card">

    <table class="highlight">
        <tr>
            <th>Song name</th>
            <th>artist</th>
            <th>album</th>
            <th>link</th>
            <th>genre</th>
         <th>rate</th>
         <th>delete</th>

        <% items.forEach(function(item){  %>
            <tr>
            <td><%=item.songname%></td>
                <td><%=item.artist%></td>
                <td><%=item.album%></td>
                <td><%=item.link%></td>
                <td><%=item.genre%></td>
                <td> <i class="material-icons">star_rate</i></td>
                <td> <a href="/delete?songname=<%=item.songname%>">
                    <i class="material-icons">delete</i>
                    </a></td>



        </tr>

        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
