<!DOCTYPE html>
<html>
<head> <% include ./head %> </head>
<body>
<% include ./header %>

    <a class="btn" href="/additem">Add New Item</a>

    <div class="card">

    <table class="highlight">
        <tr>
            <th>Song name</th><th>artist</th><th>album</th><th>link</th><th>genre</th>
                    <th></th>


        <% items.forEach(function(item){  %>
            <tr>
            <td><%=item.songname%></td><td><%=item.artist%></td><td><%=item.album%></td><td><%=item.link%></td><td><%=item.genre%></td><td>rate</td><td>delete</td>



        </tr>

        <% }) %>
    </table>

    </div>

<% include ./footer %>
</body>
</html>
